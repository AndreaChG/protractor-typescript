{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc/e2e",
    "baseUrl": "./",
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "include": [
    "e2e/src/**/*.ts",
    "e2e/test-suites/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}

